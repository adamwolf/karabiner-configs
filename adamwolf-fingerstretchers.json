{
  "title": "Squash fingerstretchers",
  "rules": [
    {
      "description": "Ignore non-ergonomic capital Y",
      "manipulators": [
        {
          "from": {
            "key_code": "y",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "out"
            },
            {
              "shell_command": "osascript -e 'beep'"
            }
          ],
          "type": "basic"
        }
      ]
